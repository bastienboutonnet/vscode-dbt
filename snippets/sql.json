{
    "Dbt Config Block": {
        "prefix": "config",
        "body": [
            "{{",
            "  config(",
            "    materialized='${1:table}'",
            "    )",
            "}}"
        ],
        "description": "Dbt Config Block"
    },
    "Dbt Incremental Config Block": {
        "prefix": "config_incremental",
        "body": [
            "{{",
            "  config(",
            "    materialized='incremental',",
            "    unique_key='${1:id}'",
            "    )",
            "}}"
        ],
        "description": "Dbt Incremental Config Block"
    },
    "Env Var": {
        "prefix": "env_var",
        "body": [
            "{{ env_var(\"${1:env_var_name}\") }}"
        ],
        "description": "Env Var"
    },
    "Load Result": {
        "prefix": "load_result",
        "body": [
            "load_result('${1:statement_name}')"
        ],
        "description": "Load Result"
    },
    "Log": {
        "prefix": "log",
        "body": [
            "{{ log(${1:var}, info=${2|True, alse|}) }}"
        ],
        "description": "Log"
    },
    "Statement": {
        "prefix": "statement",
        "body": [
            "{% call statement(${1:name}, fetch_result=${2|True,False|}) %}",
            "  ${3:select 1}",
            "{% endcall %}"
        ],
        "description": "Statement"
    },
    "Ref": {
        "prefix": "ref",
        "body": [
            "{{ ref('${1:model_name}') }}"
        ],
        "description": "Ref"
    },
    "Return": {
        "prefix": "return",
        "body": [
            "{{ return(${1}) }}'"
        ],
        "description": "Return"
    },
    "Var": {
        "prefix": "var",
        "body": [
            "{{ var('${1:var_name}') }}"
        ],
        "description": "Var"
    },
    "Source": {
        "prefix": "source",
        "body": [
            "{{ source('${1:source}', '${2:table_name}') }}"
        ],
        "description": "Source"
    }
}