{
    "name": "vscode-dbt",
    "displayName": "vscode-dbt",
    "description": "VSCode Snippet Package for jinja and dbt",
    "publisher": "bastienboutonnet",
    "icon": "images/vscode_dbt_icon.png",
    "version": "0.3.0",
    "engines": {
        "vscode": "^1.40.0"
    },
    "categories": [
        "Snippets"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "jinja-sql",
                "path": "./snippets/snippets_sql.json"
            },
            {
                "language": "jinja-yaml",
                "path": "./snippets/snippets_yaml.json"
            },
            {
                "language": "jinja-md",
                "path": "./snippets/snippets_markdown.json"
            }
        ]
    },
    "extensionDependencies": [
        "samuelcolvin.jinjahtml"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/bastienboutonnet/vscode-dbt.git"
    },
    "homepage": "https://github.com/bastienboutonnet/vscode-dbt/blob/master/README.md"
}
